<h1>Editing article</h1>
<div class="new_div">
	<ul>
	<%= form_for :quart, url: quart_path(@quart), method: :patch do |f| %>
	 
	    <li><%= f.label :name %></li>
	    <%= f.text_field :name %>
	 
	    <li><%= f.label :description %></li>
	    <%= f.text_area :description %>
	    <li><%= f.file_field :avatar %></li>

	    <li><%= f.submit %></li>
	 
	<% end %>
	</ul>
</div>

